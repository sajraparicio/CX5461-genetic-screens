number = {1},
pages = {5743},
pmid = {28720801},
title = {{Mapping and characterization of G-quadruplexes in Mycobacterium tuberculosis gene promoter regions}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28720801 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5515968 http://www.nature.com/articles/s41598-017-05867-z},
volume = {7},
