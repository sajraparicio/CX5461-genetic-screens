pmid = {28778862},
publisher = {American Association for Cancer Research},
title = {{Targeting RNA-Polymerase I in Both Chemosensitive and Chemoresistant Populations in Epithelial Ovarian Cancer}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28778862 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5668148 http://clincancerres.aacrjournals.org/lookup/doi/10.1158/1078-0432.CCR-17-0282},
