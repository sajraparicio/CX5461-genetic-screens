number = {17},
pages = {7155--60},
pmid = {19369211},
publisher = {National Academy of Sciences},
title = {{PALB2 is an integral component of the BRCA complex required for homologous recombination repair.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19369211 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2678481},
volume = {106},
