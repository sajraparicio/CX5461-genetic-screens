file = {::},
issn = {1541-7786},
journal = {Molecular Cancer Research},
month = {jul},
number = {7},
pages = {1110--1118},
pmid = {19584259},
publisher = {American Association for Cancer Research},
title = {{PALB2 Functionally Connects the Breast Cancer Susceptibility Proteins BRCA1 and BRCA2}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19584259 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4928587 http://mcr.aacrjournals.org/cgi/doi/10.1158/1541-7786.MCR-09-0123},
