issn = {0261-4189},
journal = {The EMBO Journal},
month = {jun},
number = {12},
pages = {1778--1792},
pmid = {23708797},
publisher = {EMBO Press},
title = {{Ubiquitin‐dependent recruitment of the Bloom Syndrome helicase upon replication stress is required to suppress homologous recombination}},
url = {http://emboj.embopress.org/content/32/12/1778.long},
volume = {32},
year = {2013}
}
